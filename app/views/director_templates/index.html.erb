<h1> List of all directors</h1>

<table border="1">

<% @list_of_directors.each do |thing| %>

<tr>
<td> <%= thing.id %> </td>
<td> 
<img src ="https://robohash.org/<%= thing.name%>" >
</td>
<td> <%= thing.name %> </td>
<td> <%= thing.dob %> </td>
<td> <a href = "/directors/<%= thing.id %>"> Show details </td>
</tr>

<% end %>
</table>
